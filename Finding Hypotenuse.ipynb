   "source": [
    "def findHypotenuse(side1, side2):\n",
    " \n",
    "    h = (((side1 * side1) + (side2 * side2))**(1/2));\n",
    "    return h;\n",
    " \n",
    "\n",
    "side1 = int(input(\"Value of Side 1 : \"));\n",
    "side2 = int(input(\"Value of Side 2 : \"));\n",
    " \n",
    "print(\"Value of hypotenuse : \",findHypotenuse(side1, side2));"
